{
  "name": "dashboard",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm install && npm run licenseCheck && npm run getDashboardVersion && vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "getDashboardVersion": "node ../scripts/getDashboardVersion.js",
    "licenseDepCheck": "license-checker --excludePrivatePackages --production --onlyAllow='MIT;ISC;Apache-2.0;0BSD;BSD-2-Clause;BSD-3-Clause;BlueOak-1.0.0;Unlicense;Custom: https://travis-ci.org/component/emitter.png' --summary",
    "licenseCheck": "npm run licenseDepCheck",
    "removeServiceResources": "rm -rf ../service/src/main/resources/public/assets && rm -rf ../service/src/main/resources/public/themes && rm ../service/src/main/resources/public/index.html && rm ../service/src/main/resources/public/skilltree.ico",
    "copyDistToService": "cp -rT dist ../service/src/main/resources/public/ && cp -rT dist ../service/target/classes/public/",
    "deploy": "npm install && npm run build && npm run copyDistToService"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.4",
    "@primevue/themes": "4.2.5",
    "@skilltree/skills-client-js": "3.6.1",
    "@toast-ui/editor": "3.2.2",
    "@tsparticles/confetti": "3.7.1",
    "@vue-a11y/announcer": "3.1.5",
    "@vueuse/core": "10.9.0",
    "apexcharts": "4.0.0",
    "axios": "1.7.4",
    "dayjs": "1.11.11",
    "deep-equal": "2.2.3",
    "dompurify": "3.1.3",
    "enquire.js": "2.1.6",
    "idb-keyval": "6.2.1",
    "loglevel": "1.9.1",
    "material-icons": "0.7.6",
    "node-emoji": "2.1.3",
    "number-format.js": "2.0.9",
    "pdfkit": "0.15.0",
    "pinia": "2.1.7",
    "postmate": "1.5.2",
    "primevue": "4.2.5",
    "sortablejs": "1.15.2",
    "tailwindcss-primeui": "0.4.0",
    "tinycolor2": "1.6.0",
    "tui-editor-plugin-font-size": "1.0.4",
    "vee-validate": "4.12.8",
    "video.js": "8.17.4",
    "vis-network": "9.1.9",
    "vue": "3.5.13",
    "vue-router": "4.5.0",
    "vue3-apexcharts": "1.7.0",
    "yup": "1.4.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.7.2",
    "@vitejs/plugin-vue": "5.2.1",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/test-utils": "2.4.5",
    "autoprefixer": "10.4.20",
    "eslint": "8.57.0",
    "eslint-plugin-vue": "9.23.0",
    "jsdom": "24.0.0",
    "license-checker": "25.0.1",
    "postcss": "8.5.1",
    "prettier": "3.2.5",
    "rollup-plugin-visualizer": "5.12.0",
    "tailwindcss": "3.4.17",
    "vite": "6.0.11",
    "vitest": "3.0.3"
  }
}
