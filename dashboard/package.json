{
  "name": "dashboard",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "description": "SkillTree Dashboard UI",
  "author": "SkillTree Team",
  "private": "true",
  "scripts": {
    "serve": "npm run setup:common && vue-cli-service serve",
    "licenseDepCheck": "license-checker --excludePrivatePackages --production --onlyAllow='MIT;ISC;Apache-2.0;BSD-2-Clause;BSD-3-Clause;Unlicense;Custom: https://travis-ci.org/component/emitter.png' --summary",
    "licenseHeaderCheck": "license-check-and-add check -f '../license-add/license-add-config.json'",
    "licenseHeaderAdd": "license-check-and-add add -f ../license-add/license-add-config.json",
    "licenseCheck": "npm run licenseHeaderCheck && npm run licenseDepCheck",
    "build": "npm run setup:common && npm run licenseCheck && npm run test:unit && vue-cli-service build",
    "deploy": "npm run build && rm -rf ../service/src/main/resources/public/static/js && cp -rT dist ../service/src/main/resources/public/ && cp -rT dist ../service/target/classes/public/",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "setup:common": "npx symlink-dir ../common-components/src/common src/common-components",
    "getDashboardVersion": "node ../scripts/getDashboardVersion.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.3",
    "@skilltree/skills-client-vue": "3.5.0",
    "@toast-ui/editor": "3.2.1",
    "@toast-ui/vue-editor": "3.2.2",
    "@vue-a11y/announcer": "2.1.0",
    "animate.css": "4.1.1",
    "apexcharts": "3.27.2",
    "axios": "0.27.2",
    "babel-polyfill": "6.26.0",
    "bootstrap": "4.6.2",
    "bootstrap-vue": "2.23.1",
    "core-js": "3.15.2",
    "dayjs": "1.10.6",
    "dompurify": "2.3.1",
    "enquire.js": "2.1.6",
    "font-awesome-picker": "2.0.0",
    "idb-keyval": "6.2.0",
    "lodash.debounce": "4.0.8",
    "matchmedia-polyfill": "0.3.2",
    "material-icons": "0.7.6",
    "node-emoji": "1.11.0",
    "number-format.js": "2.0.9",
    "sockjs-client": "1.5.1",
    "sortablejs": "1.14.0",
    "tui-editor-plugin-font-size": "1.0.4",
    "vee-validate": "3.4.14",
    "vis-data": "7.1.2",
    "vis-network": "9.0.4",
    "vue": "2.6.14",
    "vue-apexcharts": "1.6.1",
    "vue-js-toggle-button": "1.3.3",
    "vue-radial-progress": "0.3.2",
    "vue-router": "3.4.9",
    "vue-scrollto": "2.20.0",
    "vue-select": "3.18.3",
    "vue-simple-progress": "1.1.1",
    "vue-simple-spinner": "1.2.10",
    "vue-virtual-scroll-list": "2.3.2",
    "vuejs-datepicker": "1.6.2",
    "vuex": "3.6.2",
    "webstomp-client": "1.2.6"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/plugin-proposal-optional-chaining": "7.14.5",
    "@vue/cli-plugin-babel": "4.5.13",
    "@vue/cli-plugin-e2e-nightwatch": "4.5.13",
    "@vue/cli-plugin-eslint": "4.5.13",
    "@vue/cli-plugin-unit-jest": "4.5.13",
    "@vue/cli-service": "4.5.13",
    "@vue/eslint-config-airbnb": "5.3.0",
    "@vue/test-utils": "1.2.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.0.6",
    "error-stack-parser": "2.0.7",
    "eslint": "7.31.0",
    "eslint-plugin-vue": "7.14.0",
    "license-check-and-add": "4.0.2",
    "license-checker": "25.0.1",
    "moment-timezone": "0.5.37",
    "sass": "1.49.9",
    "sass-loader": "10.2.0",
    "symlink-dir": "^5.0.1",
    "vue-cli-plugin-webpack-bundle-analyzer": "4.0.0",
    "vue-template-compiler": "2.6.14"
  }
}
