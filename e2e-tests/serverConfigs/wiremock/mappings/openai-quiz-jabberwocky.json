{
  "priority": 1,
  "request": {
    "method": "POST",
    "urlPath": "/v1/chat/completions",
    "bodyPatterns": [
        { "contains": "invalidquiz" },
        { "contains": "Quiz Generation" },
        { "doesNotContain": "Modify an existing quiz" }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "text/event-stream"
    },
    "bodyFileName": "response-body-quiz-jabberwocky.txt",
    "transformers": ["response-template"],
    "chunkedDribbleDelay": {
      "numberOfChunks": 15,
      "totalDuration": 2000
    }
  }
}