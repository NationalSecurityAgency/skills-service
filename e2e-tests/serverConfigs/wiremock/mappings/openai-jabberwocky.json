{
  "priority": 1,
  "request": {
    "method": "POST",
    "urlPath": "/v1/chat/completions",
    "bodyPatterns": [
      {
        "contains": "jabberwocky",
        "doesNotContain": "Quiz Generation",
        "doesNotContain": "Multiple Choice",
        "doesNotContain": "Multiple Answers",
        "doesNotContain": "Text Input"
      }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "text/event-stream"
    },
    "bodyFileName": "response-body-jabberwocky.txt",
    "transformers": ["response-template"],
    "chunkedDribbleDelay": {
      "numberOfChunks": 15,
      "totalDuration": 2000
    }
  }
}