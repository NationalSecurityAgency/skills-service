{
  "priority": 1,
  "request": {
    "method": "POST",
    "urlPath": "/v1/chat/completions",
    "bodyPatterns": [
      { "contains": "chess" },
      { "doesNotContain": "current description" },
      { "doesNotContain": "Quiz Generation" }
    ]
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "text/event-stream"
    },
    "bodyFileName": "response-body-chess.txt",
    "transformers": ["response-template"],
    "chunkedDribbleDelay": {
      "numberOfChunks": 20,
      "totalDuration": 1500
    }
  }
}